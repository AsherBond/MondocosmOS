inline TCollection_SeqNode::TCollection_SeqNode(const TCollection_SeqNodePtr& n,const TCollection_SeqNodePtr& p ) 
: myNext(n),myPrevious(p)
{
}

inline TCollection_SeqNodePtr& TCollection_SeqNode::Next() const
{
