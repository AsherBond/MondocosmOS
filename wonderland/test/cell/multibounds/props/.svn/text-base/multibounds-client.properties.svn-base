
# Properties for running the multibounds client test.  Values that are not
# set will be assigned these default values.

# the server to connect to
sgs.server=localhost

# the port to connect to
sgs.port=1139

# the user name to log in with
multibounds.username=foo

# the number of groups to create
multibounds.group.num=4

# the number of clients in each group
multibounds.group.size=4

# the distance between groups
multibounds.group.separation=15

# the percentage of time each client is idle
multibounds.idle=0

# coordinates of inital group
multibounds.x=0
multibounds.y=0
multibounds.z=9
