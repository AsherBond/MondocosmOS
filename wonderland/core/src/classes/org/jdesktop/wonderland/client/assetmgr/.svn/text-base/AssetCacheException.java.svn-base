package org.jdesktop.wonderland.client.assetmgr;

import org.jdesktop.wonderland.common.InternalAPI;

/**
 * An exception when an update to the cache has failed.
 * 
 * @author Jordan Slott <jslott@dev.java.net>
 */
@InternalAPI
public class AssetCacheException extends Exception {

    /**
     * Creates a new instance of <code>AssetCacheException</code> without detail message.
     */
    public AssetCacheException() {
    }


    /**
     * Constructs an instance of <code>AssetCacheException</code> with the specified detail message.
     * @param msg the detail message.
     */
    public AssetCacheException(String msg) {
        super(msg);
    }
}
