<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="WonderlandGroupPU" transaction-type="JTA">
    <jta-data-source>jdbc/wonderlanddb</jta-data-source>
    <class>org.jdesktop.wonderland.modules.securitygroups.weblib.db.GroupEntity</class>
    <class>org.jdesktop.wonderland.modules.securitygroups.weblib.db.MemberEntity</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties>
      <property name="eclipselink.ddl-generation" value="create-tables"/>
    </properties>
  </persistence-unit>
</persistence>
