package org.jdesktop.wonderland.modules.sharedstate.common;
import org.jdesktop.wonderland.common.cell.state.CellState;
import java.io.Serializable;

/**
 * The base class for data shared via the shared state component.  In order
 * to be used in a shared map, data types must extend SharedData, be properly
 * serializable via JAXB (ie annotated with @XmlRootElement) and be
 * annotated with @ServerState.
 * 
 * @author Jonathan Kaplan <jonathankap@gmail.com>
 * @author jagwire
 */
public abstract class SharedData implements Serializable, CellState {
}
