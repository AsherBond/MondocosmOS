<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>wonderland</name>
            <properties>
                <property name="project.license">gplv2</property>
            </properties>
            <folders>
                <source-folder>
                    <label>core/src/classes</label>
                    <type>java</type>
                    <location>core/src/classes</location>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <target>release</target>
                </action>
                <action name="clean">
                    <target>clean</target>
                </action>
                <action name="rebuild">
                    <target>clean</target>
                    <target>release</target>
                </action>
                <action name="javadoc">
                    <target>javadoc</target>
                </action>
            </ide-actions>
            <export>
                <type>folder</type>
                <location>dist</location>
                <build-target>release</build-target>
            </export>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>core/src/classes</label>
                        <location>core/src/classes</location>
                    </source-folder>
                    <source-file>
                        <location>build.xml</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="javadoc"/>
                    <action>
                        <label>Run Server</label>
                        <target>run-server</target>
                    </action>
                    <action>
                        <label>Debug Server</label>
                        <target>debug-server</target>
                    </action>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/1">
            <compilation-unit>
                <package-root>core/src/classes</package-root>
                <built-to>dist</built-to>
                <source-level>1.5</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
